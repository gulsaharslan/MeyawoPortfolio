
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@Html.Partial("/Views/Default/HeadPartial.cshtml")

<body data-spy="scroll" data-target=".navbar" data-offset="40" id="home">

    <!-- Page Navbar -->
    @Html.Partial("/Views/Default/NavbarPartial.cshtml")
    <!-- End of Page Navbar -->

    <!-- page header -->
    @Html.Action("FeaturePartial")
    <!-- end of page header -->

    <!-- about section -->
    <section class="section pt-0" id="about">
        <!-- container -->
        <div class="container text-center">
            <!-- about wrapper -->
            @Html.Action("AboutPartial")
            <!-- end of about wrapper -->

        </div><!-- end of container -->

    </section> <!-- end of about section -->

    <!-- service section -->
    @Html.Action("ServicePartial")
    <!-- end of service section -->

    <!-- portfolio section -->
    @Html.Action("ProjectsPartial")
    <!-- end of portfolio section -->

   
    <!-- testimonial section -->
    @Html.Action("TestimonialPartial")
    <!-- end of testimonial section -->

    <!-- contact section -->
    @Html.Action("ContactPartial")
    <!-- end of contact section -->

    <!-- footer -->
    @Html.Partial("/Views/Default/FooterPartial.cshtml")
    <!-- end of page footer -->

    <!-- core  -->
    <script src="~/Templates/meyawo-1.0.0/assets/vendors/jquery/jquery-3.4.1.js"></script>
    <script src="~/Templates/meyawo-1.0.0/assets/vendors/bootstrap/bootstrap.bundle.js"></script>

    <!-- bootstrap 3 affix -->
    <script src="~/Templates/meyawo-1.0.0/assets/vendors/bootstrap/bootstrap.affix.js"></script>

    <!-- Meyawo js -->
    <script src="~/Templates/meyawo-1.0.0/assets/js/meyawo.js"></script>

</body>

</html>